{
  "compilerOptions": {
    "target": "ESNext",
    "module": "esnext",
    "strict": true,
    "useDefineForClassFields": true,
    "resolveJsonModule": true,
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@external/data-manager/*": ["../soie/packages/data-manager/src/*"],
      "@external/data-manager": ["../soie/packages/data-manager/src/index.ts"],
      "@external/fetcher/*": ["../soie/packages/fetcher/src/*"],
      "@external/fetcher": ["../soie/packages/fetcher/src/index.ts"]
    }
  },
  "include": ["**/*.ts", ".log/**/*.ts", "demo/time/callback/index.js"],
  "exclude": ["node_modules/"]
}
